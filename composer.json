{
    "name": "spatie/laravel-rate-limited-job-middleware",
    "description": "A middleware that can rate limit jobs",
    "keywords": [
        "spatie",
        "laravel-rate-limited-job-middleware"
    ],
    "homepage": "https://github.com/spatie/laravel-rate-limited-job-middleware",
    "license": "MIT",
    "authors": [
        {
            "name": "Freek Van der Herten",
            "email": "freek@spatie.be",
            "homepage": "https://spatie.be",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "artisansdk/ratelimiter": "^1.0@RC",
        "illuminate/cache": "^8.0|^9.0",
        "illuminate/redis": "^8.0|^9.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.4",
        "orchestra/testbench": "^6.0|^7.0",
        "pestphp/pest": "^1.18",
        "pestphp/pest-plugin-laravel": "^1.1",
        "phpunit/phpunit": "^8.2|^9.0",
        "spatie/pest-plugin-test-time": "^1.0",
        "symfony/var-dumper": "^5.0|^6.0"
    },
    "autoload": {
        "psr-4": {
            "Spatie\\RateLimitedMiddleware\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Spatie\\RateLimitedMiddleware\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage-html coverage"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
